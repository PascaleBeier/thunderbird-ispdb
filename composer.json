{
    "name": "pascalebeier/thunderbird-ispdb",
    "type": "library",
    "description": "Auto-generated ISPDB distribution from Thunderbird for PHP Applications.",
    "keywords": [
        "thunderbird-ispdb",
        "autoconfig",
        "ispdb",
        "imap",
        "smtp",
        "pop3"
    ],
    "homepage": "https://github.com/pascalebeier/thunderbird-ispdb",
    "license": "MIT",
    "authors": [
        {
            "name": "Pascale Beier",
            "email": "mail@pascalebeier.de",
            "homepage": "https://pascalebeier.de",
            "role": "Developer"
        }
    ],
    "extra": {
        "branch-alias": {
            "dev-master": "1.0-dev"
        }
    },
    "autoload": {
        "psr-4": {
            "PascaleBeier\\ThunderbirdIspdb\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "PascaleBeier\\ThunderbirdIspdb\\": "bin/src/"
        }
    },
    "scripts": {
        "check-style": "phpcs src bin",
        "fix-style": "phpcbf src bin"
    },
    "require": {
        "php": ">=7.1"
    },
    "require-dev": {
        "php": ">=8.0",
        "symfony/console": "6.3.0",
        "symfony/http-client": "6.3.1",
        "symfony/process": "6.3.0",
        "squizlabs/php_codesniffer": "3.7.2"
    }
}
